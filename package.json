{
  "name": "FunDub",
  "version": "0.0.0",
  "description": "A simple app to allow watching youtube video together",
  "scripts": {
    "clean:lib": "rimraf lib",
    "clean:dist": "rimraf public/dist",
    "clean": "npm run clean:lib && npm run clean:dist",
    "build:babel": "cross-env NODE_ENV=production babel src -d lib --ignore 'app'",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:babel && npm run build:webpack",
    "start": "babel-node devServer.js",
    "now-start": "node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HarisUa/FunDub.git"
  },
  "author": "Zill Ding",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^1.0.7",
    "eventsource-polyfill": "^0.9.6",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.11.1",
    "qs": "^6.1.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-github-fork-ribbon": "^0.4.4",
    "react-notification-system": "^0.2.7",
    "react-redux": "^4.4.5",
    "react-timeago": "^3.0.0",
    "react-tooltip": "^2.0.1",
    "react-youtube": "^6.1.0",
    "redux": "^3.5.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.4.3",
    "socket.io-client": "^1.4.5",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "express": "^4.13.3",
    "immutable": "^3.8.1",
    "md5": "^2.1.0",
    "react-fontawesome": "^1.6.1",
    "react-google-login": "^3.0.10",
    "react-native-radio-buttons": "^1.0.0",
    "socket.io": "^1.4.5"
  }
}
